{
  "name": "yaml-conf",
  "version": "0.0.2",
  "description": "Simple yaml-based configuration utility",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/sripberger/yaml-conf.git"
  },
  "keywords": [
    "yaml",
    "conf",
    "config",
    "configuration"
  ],
  "scripts": {
    "lint": "eslint . || exit 0",
    "unit": "mocha ./test/setup.js ./test/unit --recursive -R spec || exit 0",
    "integration": "mocha ./test/setup.js ./test/integration --recursive -R spec || exit 0",
    "test": "npm run unit; npm run integration",
    "docs": "documentation build -f html -o docs"
  },
  "author": "Steve Ripberger",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.5.0",
    "documentation": "^5.2.2",
    "eslint": "^3.19.0",
    "mocha": "^3.4.2",
    "rimraf": "^2.6.2",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  },
  "dependencies": {
    "fs-extra": "^4.0.2",
    "js-yaml": "^3.10.0",
    "lodash": "^4.17.4",
    "xerror": "^1.1.2"
  }
}
